*{
	margin: 0;
	padding: 0;
	font-family: "微软雅黑";
	font-size: 15px;
}
a{
	text-decoration: none;
	color: #777;
}
img{
	/*vertical-align: top;*/
}
a:hover{
	color: deepskyblue;
}
ul li{
	list-style: none;
}
.container{
	width: 100%;
	margin-bottom: 30px;
}
.contant{
	width: 1024px;
	margin: 0 auto;
}
.quan{
	width: 100%;
	background: #eee;
	border-bottom: 1px solid #bbb;
}
.contant1 ul li{
	display: inline-block;
}
/*.contant ul li a{
	display: inline-block;
	padding: 5px 0;
}*/
.contant1 ul{
	display: inline-block;
}
.contant1 ul:first-child li{
	border-right: 1px solid #ddd;
	padding: 5px;
}
.contant1 ul:last-child{
	margin-left: 31%;
}
.contant1 ul:last-child li a{
	border-left: 2px solid #BBBBBB;
	padding: 0 10px;
}
.contant1 ul:last-child li:first-child a{
	border: 0;
}
.contant1 ul:last-child li img{
	margin-right: 8px;
}
.login{
	position: absolute;
	background: #fff;
	display: none;
	box-shadow: 3px 3px 1px #bbb;
	padding: 10px;
}
.login span{
	display: block;
	padding: 10px;
	border: 1px solid #bbb;
	margin-bottom: 10px;
}
.login span:hover{
	border: 1px solid deepskyblue;
	color: #00BFFF;
	cursor: pointer;
}

.contant2 ul li{
	display: inline-block;
}
.contant2 ul li span{
	display: inline-block;
	width: 0px;
	height: 0px;
	border: 5px solid transparent;
	border-top: 5px solid #444;
}
.contant2 ul li b{
	margin-left: 20px;
}
.contant2 ul li span{
	cursor: pointer;
}
.contant2 ul li input{
	width: 400px;
	height: 35px;
}
.contant2 ul li button{
	width: 80px;
	height: 37px;
	background: dodgerblue;
	border: 0;
	border-radius: 3px;
	color: white;
	font-size: 18px;
}
.contant2 .search{
	margin-left: 22%;
}
.contant2 .search ul li{
	margin-top: 10px;
	margin-left: 10px;
}
.addr{
	background: white;
	position: absolute;
	box-shadow: 2px 2px 1px #ccc;
	padding: 10px;
	display: none;
}
.addr .return{
	cursor: pointer;
}
.addr .return:hover{
	color: dodgerblue;
}
.province ul li{
	margin-top: 10px;
}
.province ul li b{
	color: red;
}
.province{
	display: none;
}

.contant3 ul li{
	display: inline-block;
}
.contant3>ul{
	background: #eee;
}
.contant3>ul li{
	border-left: 1px solid white;
}
.contant3>ul li:first-child{
	border-top: 2px solid #1E90FF;
}
.contant3>ul li:first-child a{
	color: #1E90FF;
}
.contant3>ul li a{
	display: inline-block;
	font-size: 17px;
	padding: 10px 65px;
	/*font-weight: 500;*/
}
.contant3 .nav{
	width: 1024px;
	position: absolute;
	background: white;
	box-shadow: 0px 0px 3px #ccc;
	display: none;
}
.contant3 .nav>ul>li{
	display: none;
}
.contant3 .nav ul li ul li a{
	display: inline-block;
	padding: 10px 35px;
}

.contant4 ul li{
	float: left;
}
.contant4 ul{
	overflow: hidden;
}
.contant4>ul>li{
	margin: 5px;
}
.contant4 .lunbo img{
	width: 630px;
	height: 318px;
}
.contant4 .left{
	border: 1px solid #aaa;
	padding: 10px 20px;
}
.contant4 .left p{
	padding: 8px 0;
	border-bottom: 1px solid #ddd;
	cursor: pointer;
}
.contant4 .left p:hover{
	color: #00BFFF;
}
.contant4 .left p img{
	margin: 10px 5px 0 5px;
}
.contant4 .right{
	border: 1px solid #aaa;
	padding:  10px 8px;
	height: 298px;
}
.contant4 .right *{
	font-size: 12px;
}
.contant4 ul li{
	margin: 0 4px;
}
.contant4 .right>p{
	margin-top: 15px;
	text-align: center;
}
.contant4 .right>p input{
	width: 185px;
	height: 25px;
	border: 1px solid #aaa;
	border-radius: 3PX;
}
.contant4 .right .bor span{
	cursor: pointer;
	display: inline-block;
	padding: 2px;
	border: 1px solid #bbb;
	border-radius: 3px;
}
.contant4 .right .bor .yibai{
	border: transparent solid 1px;
	background: #e40077;
}
.contant4 .right .bor span:hover{
	border: transparent solid 1px;
	background: #e40077;
}
.contant4 .right>p button{
	background: deepskyblue;
	color: white;
	width: 70px;
	height: 30px;
	border: 0;
	border-radius: 3px;
}
.contant4 .right ul li{
	margin: 7px 0;
}
.contant4 .right ul li img{
	position: relative;
}
/*.contant4 .right ul li:hover img{
	width: 33px;
	height: 33px;
}*/
#banner
	{
		width: 630px;
		height: 318px;
		margin:0px auto;
		overflow:hidden;
		z-index:-1;
	}
#btn{	display: inline;
		width:200px;
		hegiht:18px;
		float:left;
		margin:-30px 0px 0px 465px;
		position:absolute;
		z-index:1;
}
#btn a{
		color:#36f;
		font:700 11px "Arial";
		background:url(../img/ibbtn.gif) no-repeat;
		float:left;
		width:18px;
		height:18px;
		line-height:18px;
		text-align:center;
		text-decoration:none;
		margin-left:5px;
}
#btn a.activeSlide{	
		color:#fff;
		background:url(../img/ibhbtn.gif) no-repeat;
}
#btn a:hover{	
		color:#fff;
		background:url(../img/ibhbtn.gif) no-repeat;
}

.contant5{
	border: 1px solid #ccc;
	color: #999;
}
.contant5>p{
	color: #00BFFF;
	font-size: 19px;
	font-weight: bold;
	padding: 10px;
}
.contant5 ul{
	overflow: hidden;
}
.contant5 ul li{
	float: left;
}
.contant5 ul li:first-child{
	border-left: 0;
}
.contant5 ul li ul li p{
	font-size: 16px;
	margin-bottom: 13px;
}
.contant5 ul li ul li span{
	display: block;
}
.contant5 ul li ul li{
	width: 236px;
	padding-left: 5px;
	border-left: 1px solid #ccc;
	border-top: 1px solid #ccc;
}
.contant5 ul li ul li img{
	position: relative;
	left: 10px;
}
/*.contant5 ul li ul li:hover img{
	left: 2px;
}*/

.contant6{
	border: 1px solid #ccc;
	color: #444444;
}
.contant6 p{
	padding: 10px;
	border-bottom: 1px solid #CCCCCC;
}
.contant6 ul{
	overflow: hidden;
}
.contant6 ul li{
	float: left;
	border-right: 1px solid #CCCCCC;
	width: 249px;
}
.contant6 ul li img{
	position: relative;
	left: 20px;
}
.contant6 ul li:last-child{
	border: 0;
}
.contant6 ul li span{
	display: block;
	margin-bottom: 10px;
	margin-left: 5px;
	margin-top: 5px;
}
/*.contant6 ul li:hover img{
	left: 10px;
}*/

.contant7{
	border: 1px solid #ccc;
	color: #999;
}
.contant7>p{
	color: #00BFFF;
	font-size: 19px;
	font-weight: bold;
	padding: 10px;
}
.contant7>ul>li:first-child img{
	border-top: 1px solid #ccc;
	border-right: 1px solid #ccc;
}
.contant7 ul{
	overflow: hidden;
}
.contant7 ul li{
	float: left;
}
.contant7 ul li:first-child{
	border-left: 0;
}
.contant7 ul li ul li p{
	font-size: 17px;
	margin-bottom: 13px;
}
.contant7 ul li ul li span{
	display: block;
}
.contant7 ul li ul li{
	width: 234px;
	padding-left: 6px;
	border-left: 1px solid #ccc;
	border-top: 1px solid #ccc;
}
.contant7 ul li ul li img{
	position: relative;
	left: 10px;
}
/*.contant7 ul li ul li:hover img{
	left: 2px;
}*/

.contant8{
	overflow: hidden;
	border: 1px solid #ccc;
}
.contant8 ul{
	float: left;
	margin: 20px 50px;
}
.contant8 ul:first-child{
	margin-left: 60px;
}
.contant8 ul:last-child{
	margin-right: 0;
}
.contant8 ul li:first-child{
	color: #333;
	font-size: 18px;
	margin-bottom: 8px;
}
.contant8 ul li a{
	font-size: 17px;
	display: inline-block;
	margin-bottom: 10px;
}

.contant9 p{
	text-align: center;
	margin-bottom: 20px;
	color: #555;
}
.contant9 p img{
	vertical-align: middle;
	margin-left: 15px;
	margin-right: 10px;
}
