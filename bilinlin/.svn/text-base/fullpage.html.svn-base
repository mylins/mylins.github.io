<link rel="stylesheet" type="text/css" href="css/jquery.fullPage.css"/>
<link rel="stylesheet" href="css/fullpage.css">
<!--<script src="http://cdn.staticfile.org/jquery/1.8.3/jquery.min.js"></script>-->
<!--<script src="http://lib.sinaapp.com/js/jquery/1.9.1/jquery-1.9.1.min.js"></script>-->
<script>
	window.jQuery || document.write("<script src='js/jquery.min1.js'><\/script>");
</script>
<!--<script src="http://cdn.staticfile.org/jqueryui/1.10.3/jquery-ui.min.js"></script>-->
<script src="js/jquery-ui.min.js" type="text/javascript" charset="utf-8"></script>
<!--<script src="http://www.dowebok.com/demo/2014/77/js/jquery.fullPage.min.js"></script>-->
<script src="js/jquery.fullPage.min.js" type="text/javascript" charset="utf-8"></script>
<script>
$(document).ready(function() {
	$.fn.fullpage({
		slidesColor: ['', '#E9E9E9', '', '', '#F9F3DC'],
		anchors: ['page1', 'page2', 'page3', 'page4', 'page5'],
		menu: '#menu',
		afterRender: function(){
			$('.inner a').each(function(){
				var $rel = $(this).attr('rel');
				var $arr = $rel.split(',');
				$(this).animate({
					left: $arr[0] + 'px',
					top: $arr[1] + 'px'
				}, 500);
			});
		},
		afterLoad: function(anchorLink, index){
			if(index == 2){
				$('.inner').eq(index - 2).find('a').each(function(){
					var $rel = $(this).attr('rel');
					var $arr = $rel.split(',');
					$(this).animate({
						left: $arr[2] + 'px',
						top: $arr[3] + 'px'
					}, 500);
				});
			}
			if(index == 3){
				$('.section3').find('p').delay(500).animate({
					left: '0'
				}, 1500, 'easeOutExpo');
			}
			if(index == 4){
				$('.section2').find('p').delay(500).animate({
					bottom: '0'
				}, 1500, 'easeOutExpo');
			}
			if(index == 5){
				$('.zanzhu-con a').fadeIn(1000);
			}
		},
		onLeave: function(index, direction){
			if(index == 2){
				$('.inner').eq(index - 2).find('a').each(function(){
					var $rel = $(this).attr('rel');
					var $arr = $rel.split(',');
					$(this).animate({
						left: $arr[0] + 'px',
						top: $arr[1] + 'px'
					}, 500);
				});
			}
			if(index == 3){
				$('.section3').find('p').delay(500).animate({
					left: '-120%'
				}, 1500, 'easeOutExpo');
			}
			if(index == 4){
				$('.section2').find('p').delay(500).animate({
					bottom: '-120%'
				}, 1500, 'easeOutExpo');
			}
			if(index == 5){
				$('.zanzhu-con a').fadeOut(1000);
			}
		}
	});
});
</script>
</head>

